{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": [
      "ES2020"
    ], 
    "declaration": true,
    "outDir": "./lib/cjs", 
    "rootDir": "./tsc", 
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "exclude": [
    "__tests__/**/*.ts",
    "compiled/**/*.ts",
    "lib/**/*.ts",
    "./jest.config.ts"
  ],
}